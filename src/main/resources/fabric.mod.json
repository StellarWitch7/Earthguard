{
  "schemaVersion": 1,
  "id": "earthguard",
  "version": "${version}",

  "name": "Earthguard",
  "description": "A Fabric mod that adds earthy items and creatures.",
  "authors": [
    "StellarWitch7"
  ],
  "contact": {
    "sources": "https://github.com/StellarWitch7/Earthguard"
  },

  "license": "ARR",
  "icon": "assets/earthguard/icon.png",

  "environment": "*",
  "entrypoints": {
    "main": [
      "com.github.stellarwitch7.earthguard.EarthguardMod"
    ],
    "client": [
      "com.github.stellarwitch7.earthguard.EarthguardClient"
    ],
    "server": [
      "com.github.stellarwitch7.earthguard.EarthguardServer"
    ],
    "fabric-datagen": [
      "com.github.stellarwitch7.earthguard.EarthguardDataGenerator"
    ]
  },
  "mixins": [
    "earthguard.mixins.json"
  ],

  "depends": {
    "fabricloader": ">=0.14.19",
    "fabric-api": "*",
    "minecraft": "~1.19.2",
    "java": ">=17"
  }
}
